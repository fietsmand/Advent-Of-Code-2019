{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "no-console": 0,
        "max-len": [
            "error",
            {
                "code": 200
            }
        ],
        "import/no-named-as-default": 0,
        "import/prefer-default-export": 0,
        "no-restricted-globals": 0,
        "class-methods-use-this": 0,
        "no-bitwise": 0
    },
    "settings": {
        "import/resolver": {
            "node": {
                "paths": [
                    "src"
                ]
            }
        },
        "quotes": {
            "allowTemplateLiterals": true,
            "avoidEscape": true
        }
    }
}